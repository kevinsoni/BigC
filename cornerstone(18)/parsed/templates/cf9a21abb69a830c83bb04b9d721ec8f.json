{
  "components/common/store-logo": "<a href=\"{{urls.home}}\" class=\"header-logo__link\" data-header-logo-link>\n    <!-- <img style=\"width: 50%;\"\n        src=\"https://cdn11.bigcommerce.com/s-mvcgh2itw2/images/stencil/original/image-manager/cybermonday.gif?t=1665658447\"\n        alt=\"\"> -->\n    {{#if settings.store_logo.image}}\n    {{#if theme_settings.logo_size '===' 'original'}}\n    <img class=\"header-logo-image-unknown-size\" src=\"{{getImage settings.store_logo.image 'logo_size'}}\"\n        alt=\"{{settings.store_logo.title}}\" title=\"{{settings.store_logo.title}}\">\n    {{else}}\n    <div class=\"header-logo-image-container\">\n        <img class=\"header-logo-image\" src=\"{{getImage settings.store_logo.image 'logo_size'}}\"\n            srcset=\"{{getImageSrcset1x2x settings.store_logo.image theme_settings.logo_size}}\"\n            alt=\"{{settings.store_logo.title}}\" title=\"{{settings.store_logo.title}}\">\n    </div>\n    {{/if}}\n    {{else}}\n    <span class=\"header-logo-text\">{{settings.store_logo.title}}</span>\n    {{/if}}\n</a>"
}